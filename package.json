{
  "name": "@tomkp/ber-tlv",
  "version": "1.0.1",
  "description": "Modern TypeScript library for ASN.1 BER-TLV encoding and decoding",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-strip-types --test src/*.test.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "asn1",
    "ber",
    "tlv",
    "ber-tlv",
    "parser",
    "encoder",
    "decoder",
    "emv",
    "smartcard"
  ],
  "author": "tomkp <tom@tomkp.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomkp/tlv.git"
  },
  "homepage": "https://github.com/tomkp/tlv#readme",
  "bugs": {
    "url": "https://github.com/tomkp/tlv/issues"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
